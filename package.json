{
  "name": "wellknown",
  "version": "0.2.0",
  "description": "convert wkt to geojson",
  "main": "index.js",
  "scripts": {
    "test": "tape test/*.js",
    "test-browser": "browserify -t brfs test/wellknown.test.js | testling"
  },
  "bin": {
    "wellknown": "cli.js"
  },
  "keywords": [
    "wkt",
    "geojson",
    "maps"
  ],
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6..latest",
      "ipad/6..latest",
      "android-browser/latest"
    ]
  },
  "author": "Tom MacWright",
  "repository": {
    "type": "git",
    "url": "https://github.com/mapbox/wellknown.git"
  },
  "license": "BSD",
  "devDependencies": {
    "tape": "~2.4.2",
    "brfs": "~0.2.1",
    "testling": "~1.6.0",
    "fuzzer": "~0.1.0"
  },
  "dependencies": {
    "concat-stream": "~1.4.5",
    "minimist": "0.0.2"
  }
}
